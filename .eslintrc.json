{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ],
        "linebreak-style": 0,
        "import/extensions": [
            "error",
            "ignorePackages"
        ],
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": false
            }
        ]
    }
}